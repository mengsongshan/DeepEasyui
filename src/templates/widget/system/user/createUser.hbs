<div id="createDialog" class="crudDialog">
    <form id="defaultForm" name="defaultForm" class="form-horizontal">
        <div class="form-group">
            <label class="col-lg-3 col-md-3 col-sm-3 control-label">用户账号</label>
            <div class="col-lg-8 col-sm-8 col-md-8">
                <div class="form-group">
                    <input type="text" class="form-control" name="newusername" value="" id="newusername">
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 col-md-3 col-sm-3 control-label">真实姓名</label>
            <div class="col-lg-8 col-sm-8 col-md-8">
                <div class="form-group">
                    <input type="text" class="form-control" name="realName" value="" id="realName">
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 col-md-3 col-sm-3 control-label">描述信息</label>
            <div class="col-lg-8 col-sm-8 col-md-8">
                <div class="form-group">
                    <input type="text" class="form-control" name="description" value="" id="description">
                </div>
            </div>
        </div>
        <div class="form-group errorChg">
            <label class="col-lg-3 col-md-3 col-sm-3  control-label">用户密码</label>
            <div class="col-lg-8 col-sm-8 col-md-8">
                <div class="input-group ">
                    <input type="password" class="form-control" name="password" placeholder="请输入密码" />
                    <span class="input-group-addon openOrClose"><i class="zmdi zmdi-eye-off"></i></span>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label class="col-lg-3 col-md-3 col-sm-3 control-label">手机号码</label>
            <div class="col-lg-8 col-sm-8 col-md-8">
                <div class="form-group">
                    <input type="text" class="form-control" name="phoneNumber" value="" id="phoneNumber">
                </div>
            </div>
        </div>
    </form>
</div>